package cmsc420.meeshquest.datastructures;

/*
 *  We are requried to use an AVL Tree from an outside source.
 *  Java Program to Implement AVL Tree. Taken from SanFoundry.
 */
 
public class AVLNode {
    AVLNode left, right;
    int data;
    int height;

    /* Constructor */
    public AVLNode()
    {
        left = null;
        right = null;
        data = 0;
        height = 0;
    }
    /* Constructor */
    public AVLNode(ity n)
    {
        left = null;
        right = null;
        data = n;
        height = 0;
    }   
}
