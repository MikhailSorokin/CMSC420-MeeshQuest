package cmsc420.meeshquest.junittests;

import java.util.TreeMap;

import org.junit.Test;

import cmsc420.meeshquest.datastructures.AvlGTree;
import cmsc420.meeshquest.datastructures.StringComparator;

public class AvlGTreeTests {

	@Test
	public void testBasics() {
		AvlGTree<String, Integer> st = new AvlGTree<String, Integer>(new StringComparator(),1);
        TreeMap<String, Integer> st2 = new TreeMap<String, Integer>(new StringComparator());

        for (int i = 0; i < 10; i++) {
            String key = "key"+i;
            st.put(key, i);
        }
        for (int i = 0; i < 10; i++) {
            String key = "key"+i;
            st2.put(key, i);
        }
        for (String s : st.subMap("key7", "key2").keySet())
            System.out.println(s + " " + st.get(s)+":"+st.keySet().contains(s));
        for (String s : st2.subMap("key7", "key3").keySet())
        System.out.println(s + " " + st.get(s)+":"+st.keySet().contains(s));
        System.out.println(st.subMap("key7", "key2").equals(st2.subMap("key7", "key3")));
	}
	
}
