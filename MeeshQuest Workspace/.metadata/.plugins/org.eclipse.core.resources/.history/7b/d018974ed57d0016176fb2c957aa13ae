package cmsc420.meeshquest.datastructures;

/**
 * A Black Node holds information about the city.
 * @author Mikhail Sorokin
 *
 */
public class BlackNode extends Node {

	public Node quadrantOne, quadrantTwo, quadrantThree, quadrantFour;
	private int[] coords;
	
	public GreyNode(int xCoord, int yCoord) {
		coords = new int[2];
		coords[0] = xCoord;
		coords[1] = yCoord;
		quadrantOne = quadrantTwo = quadrantThree = quadrantFour = null;
	}
	
	public int[] getCoords() {
		return coords;
	}
	
}
