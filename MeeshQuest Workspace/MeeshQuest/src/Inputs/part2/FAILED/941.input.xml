<commands
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="part2in.xsd"
    spatialWidth="1024"
    spatialHeight="1024"
    g="3"
    pmOrder="3">
    <createCity name="Baltimore" x="140" y="37" radius="0" color="black"/>
    <createCity name="Chicago" x="133" y="34" radius="0" color="black"/>
    <createCity name="New_York" x="200" y="300" radius="0" color="black"/>
    
    <createCity name="Zurich" x="1000" y="1000" radius="0" color="black"/>
    <createCity name="Paris" x="1002" y="1000" radius="0" color="black"/>
    <createCity name="Amsterdam" x="450" y="450" radius="0" color="black"/>
    <createCity name="Blah" x="451" y="451" radius="0" color="black"/>
    
    <mapRoad start="Chicago" end="Baltimore"/>
    <mapRoad start="Paris" end="Baltimore"/>
    
    <mapRoad start="New_York" end="Amsterdam"/>
    <mapRoad start="Zurich" end="Amsterdam"/>
    
    <mapRoad start="Amsterdam" end="Chicago"/>
    <mapRoad start="Blah" end="Chicago"/>
    
    <rangeCities x="30000" y="-3000" radius="3"/>
    <rangeCities x="30000" y="-3000" radius="0"/>
    <rangeCities x="133" y="34" radius="3"/>
    <rangeCities x="450" y="450" radius="3"/>
    
    <rangeCities x="140" y="37" radius="3"/>
    <rangeRoads x="140" y="37" radius="0"/>
    <rangeRoads x="100" y="100" radius="0"/>
    <rangeRoads x="100" y="100" radius="10"/>
</commands>
