<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<commands
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="part2in.xsd"
  spatialWidth="256"
  spatialHeight="256"
  pmOrder="3"
  g="1">
  
	<createCity name="A" x="10" y="10" color="black" radius="2"/>
	<createCity name="B" x="100" y="50" color="black" radius="2"/>
	<createCity name="C" x="20" y="30" color="black" radius="2"/>
	<createCity name="E" x="200" y="100" color="black" radius="2"/>
	<createCity name="F" x="20" y="10" color="black" radius="2"/>
	<createCity name="D" x="1" y="10" color="black" radius="2"/>
	<createCity name="G" x="10" y="20" color="black" radius="2"/>

	<mapCity name="G"/>
	<mapRoad start="A" end="B"/>
	<mapRoad start="D" end="E"/>
	<mapCity name="C"/>
	<mapCity name="F"/>
	
	<rangeCities x="10" y="10" radius="10" saveMap="image"/>
	<rangeCities x="20" y="30" radius="0"/>
	<rangeCities x="20" y="10" radius="10"/>
</commands>
