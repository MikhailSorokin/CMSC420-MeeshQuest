<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<commands
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="part2in.xsd"
  spatialWidth="1024"
  spatialHeight="1024"
  pmOrder="3"
  g="2">

    <createCity name="A" x="0" y="0" color="black" radius="2"/>
	<createCity name="B" x="1024" y="1024" color="black" radius="2"/>
	<createCity name="C" x="1024" y="0" color="black" radius="2"/>
	<createCity name="D" x="0" y="1024" color="black" radius="2"/>
	<createCity name="E" x="512" y="512" color="black" radius="2"/>
	<createCity name="F" x="510" y="510" color="black" radius="2"/>
	<createCity name="G" x="514" y="514" color="black" radius="2"/>

    <mapRoad start="A" end="C"/>
    <mapRoad start="C" end="E"/>
    <mapRoad start="B" end="A"/>
    <mapRoad start="D" end="A"/>
    <mapRoad start="D" end="E"/>

    <mapRoad start="B" end="F"/>
    <mapRoad start="B" end="E"/>

    <nearestCity x="512" y="512"/>
    <nearestCity x="1024" y="0"/>
    <mapCity name="G"/>
    <printPMQuadtree/>
    <printAvlTree/>
    <shortestPath start="A" end="G"/>
    <shortestPath start="G" end="A" saveMap="a" saveHTML="b"/>
</commands>
