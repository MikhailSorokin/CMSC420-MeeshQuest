	xsi:noNamespaceSchemaLocation="part2in.xsd"
	spatialWidth="32"
	spatialHeight="16"
	g="3"
	pmOrder="3">
	
	<createCity name="A" radius="3" x="2" y="0" color="black"/>
	<createCity name="B" radius="3" x="2" y="4" color="black"/> 
	<createCity name="C" radius="3" x="7" y="9" color="black"/>
	<createCity name="D" radius="3" x="10" y="5" color="black"/> 
	<createCity name="E" radius="3" x="13" y="10" color="black"/>
	<createCity name="F" radius="3" x="17" y="10" color="black"/> 
	
	
	<mapRoad start="A" end="B"/>
	<mapRoad start="B" end="C"/>
	<mapRoad start="C" end="D"/>
	<mapRoad start="D" end="E"/>
	<mapRoad start="E" end="F"/>
	
	<saveMap name="file"/>
	
