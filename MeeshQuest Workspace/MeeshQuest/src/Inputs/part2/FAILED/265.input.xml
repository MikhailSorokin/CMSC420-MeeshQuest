<commands
	
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	
xsi:noNamespaceSchemaLocation="part2in.xsd"
 	
spatialWidth="128"
 	
spatialHeight="128"
 	
pmOrder="3"
 	
g="1">
 
    <createCity name="Baltimore" x="39" y="76" radius="30" color="black"/>
    <createCity name="Chicago" x="81" y="47" radius="35" color="black"/>
    <createCity name="Atlanta" x="84" y="33" radius="40" color="black"/>
    <createCity name="Los_Angeles" x="118" y="33" radius="80" color="black"/>
    
    <!-- error mapIsEmpty -->
    <printPMQuadtree />
    
    <!-- map and print 1 city -->
    <mapRoad start="Baltimore" end="Los_Angeles"/>
    <printPMQuadtree />
    
    <!-- map and print 2 cities -->
    <mapCity name="Chicago"/>
    <printPMQuadtree />
    
    <!-- map and print the rest -->
    <mapCity name="Los_Angeles"/>
        <mapRoad start="Los_Angeles" end="Atlanta"/>
    
    <saveMap name="test"/>
    <printPMQuadtree />
</commands>
