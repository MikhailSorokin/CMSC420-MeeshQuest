<results>
  <error type="cityDoesNotExist">
    <command name="deleteCity"/>
    <parameters>
      <name value="London"/>
    </parameters>
  </error>
  <success>
    <command name="createCity"/>
    <parameters>
      <name value="London"/>
      <x value="1000"/>
      <y value="40"/>
      <radius value="2"/>
      <color value="green"/>
    </parameters>
    <output/>
  </success>
  <success>
    <command name="listCities"/>
    <parameters>
      <sortBy value="name"/>
    </parameters>
    <output>
      <cityList>
        <city color="green" name="London" radius="2" x="1000" y="40"/>
      </cityList>
    </output>
  </success>
  <success>
    <command name="deleteCity"/>
    <parameters>
      <name value="London"/>
    </parameters>
    <output/>
  </success>
  <error type="noCitiesToList">
    <command name="listCities"/>
    <parameters>
      <sortBy value="name"/>
    </parameters>
  </error>
</results>